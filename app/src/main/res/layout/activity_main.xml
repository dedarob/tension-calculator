<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">
    <ImageView
            android:layout_width="785dp"
            android:layout_height="815dp" app:srcCompat="@drawable/scottiek" android:id="@+id/imageView7"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"/>
    <TextView
            android:text="Mass:"
            android:layout_width="122dp"
            android:layout_height="46dp" android:id="@+id/textView5"
            android:textSize="34sp" android:textColor="#FFFFFF"
            tools:ignore="MissingConstraints"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="16dp" android:layout_marginTop="10dp"
            app:layout_constraintTop_toBottomOf="@+id/textView4" app:layout_constraintEnd_toStartOf="@+id/massInput"
            android:layout_marginEnd="12dp"
            app:layout_constraintBottom_toTopOf="@+id/rodrigoGoes"/>
    <ImageView
            android:layout_width="129dp"
            android:layout_height="180dp" app:srcCompat="@drawable/goes" android:id="@+id/rodrigoGoes"
            tools:ignore="MissingConstraints"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="17dp" android:layout_marginTop="17dp"
            app:layout_constraintTop_toBottomOf="@+id/textView5" app:layout_constraintEnd_toStartOf="@+id/massInput"
            android:layout_marginEnd="16dp"
            app:layout_constraintBottom_toTopOf="@+id/imageView5"/>
    <EditText
            android:layout_width="185dp"
            android:layout_height="63dp"
            android:inputType="numberDecimal"
            android:ems="10"
            android:id="@+id/massInput"
            android:textColorHint="#D3D3D3"
            android:minHeight="48dp"
            android:hint="Ex: 1000"
            android:textSize="25sp"
            android:layout_marginStart="24dp"
            app:layout_constraintStart_toEndOf="@+id/textView5"
            app:layout_constraintTop_toBottomOf="@+id/angleInput" android:layout_marginBottom="31dp"
            app:layout_constraintBottom_toTopOf="@+id/calc" android:layout_marginEnd="64dp"
            app:layout_constraintEnd_toEndOf="parent"/>
    <Button
            android:text="Calculate"
            android:layout_width="179dp"
            android:layout_height="59dp" android:id="@+id/calc"
            android:textSize="21sp"
            tools:ignore="MissingConstraints"
            android:layout_marginStart="16dp"
            app:layout_constraintStart_toEndOf="@+id/rodrigoGoes" android:layout_marginTop="26dp"
            app:layout_constraintTop_toBottomOf="@+id/massInput" android:layout_marginEnd="70dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginBottom="30dp"
            app:layout_constraintBottom_toTopOf="@+id/textView8"/>
    <TextView
            android:text="Angle:"
            android:layout_width="121dp"
            android:layout_height="48dp" android:id="@+id/textView4"
            android:textSize="34sp" android:textColor="#FFFFFF"
            tools:ignore="MissingConstraints"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="15dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="17dp"
            app:layout_constraintBottom_toTopOf="@+id/textView5" app:layout_constraintEnd_toStartOf="@+id/angleInput"
            android:layout_marginEnd="12dp" app:layout_constraintVertical_bias="1.0"/>
    <TextView
            android:hint="Tension D"
            android:layout_width="157dp"
            android:textColorHint="#D3D3D3"
            android:layout_height="45dp" android:id="@+id/textView8"
            android:textSize="34sp" android:textColor="#FFFFFF" tools:ignore="MissingConstraints"
            android:layout_marginStart="25dp"
            app:layout_constraintStart_toEndOf="@+id/rodrigoGoes" android:layout_marginTop="1dp"
            app:layout_constraintTop_toBottomOf="@+id/calc" android:layout_marginEnd="87dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginBottom="14dp"
            app:layout_constraintBottom_toTopOf="@+id/textView9" app:layout_constraintHorizontal_bias="0.0"/>
    <TextView
            android:hint="Tension B"
            android:textColorHint="#D3D3D3"
            android:layout_width="162dp"
            android:layout_height="38dp" android:id="@+id/textView9"
            android:textSize="34sp" android:textColor="#FFFFFF" tools:ignore="MissingConstraints"
            android:layout_marginStart="12dp"
            app:layout_constraintStart_toEndOf="@+id/imageView5" app:layout_constraintTop_toBottomOf="@+id/textView8"
    />
    <EditText
            android:layout_width="191dp"
            android:layout_height="54dp"
            android:inputType="numberDecimal"
            android:ems="10"
            android:id="@+id/angleInput"
            android:minHeight="48dp"
            android:textColorHint="#D3D3D3"
            android:hint="Ex: 45"
            android:textSize="25sp"
            android:layout_marginStart="24dp"
            app:layout_constraintStart_toEndOf="@+id/textView4" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="15dp" android:layout_marginBottom="1dp"
            app:layout_constraintBottom_toTopOf="@+id/massInput" android:layout_marginEnd="58dp"
            app:layout_constraintEnd_toEndOf="parent"/>
    <ImageView
            android:layout_width="188dp"
            android:layout_height="194dp" app:srcCompat="@drawable/gosling" android:id="@+id/imageView2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView5" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/imageView6" android:layout_marginEnd="10dp"/>
    <ImageView
            android:layout_width="143dp"
            android:layout_height="154dp" app:srcCompat="@drawable/uncle" android:id="@+id/imageView5"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="16dp" android:layout_marginBottom="40dp"
            app:layout_constraintBottom_toTopOf="@+id/imageView2"
            app:layout_constraintTop_toBottomOf="@+id/rodrigoGoes"/>
    <ImageView
            android:layout_width="197dp"
            android:layout_height="125dp" app:srcCompat="@drawable/jeeves" android:id="@+id/imageView6"
            android:layout_marginEnd="16dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginStart="10dp" app:layout_constraintStart_toEndOf="@+id/imageView2"/>
    <ImageView
            android:layout_width="212dp"
            android:layout_height="195dp" app:srcCompat="@drawable/ex" android:id="@+id/imageView9"
            android:layout_marginTop="15dp"
            app:layout_constraintTop_toBottomOf="@+id/textView9" app:layout_constraintStart_toEndOf="@+id/imageView2"
            android:layout_marginBottom="33dp" app:layout_constraintBottom_toTopOf="@+id/imageView6"
    />

</androidx.constraintlayout.widget.ConstraintLayout>